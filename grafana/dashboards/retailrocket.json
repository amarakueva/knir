          "format": "time_series",
          "rawQuery": true,
          "rawSql": "SET max_parallel_workers_per_gather = 0;\nWITH base AS (\n  SELECT\n    fid.date_key,\n    SUM(fid.views) AS views,\n    SUM(fid.add_to_cart) AS add_to_cart,\n    SUM(fid.transactions) AS transactions\n  FROM dm.fact_item_daily fid\n  LEFT JOIN dm.dim_item di ON di.item_id = fid.item_id\n  WHERE $__timeFilter(fid.date_key)\n    AND COALESCE(di.parent_category_id::text, 'UNCATEGORIZED') ~ 'UNCATEGORIZED'\n  GROUP BY fid.date_key\n)\nSELECT\n  $__time(base.date_key),\n  stages.stage AS metric,\n  stages.value\nFROM base\nCROSS JOIN LATERAL (\n  VALUES\n    ('Просмотры', base.views),\n    ('Добавления в корзину', base.add_to_cart),\n    ('Транзакции', base.transactions)\n) AS stages(stage, value)\nORDER BY 1, 2;\n",
